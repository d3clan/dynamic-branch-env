# GitHub OAuth credentials
GITHUB_CLIENT_ID=your_github_oauth_client_id
GITHUB_CLIENT_SECRET=your_github_oauth_client_secret

# NextAuth configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_random_secret_at_least_32_chars

# GitHub organization (optional - for membership restriction)
GITHUB_ORG_NAME=your_org_name

# AWS Configuration
AWS_REGION=eu-west-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# DynamoDB Tables
ENVIRONMENTS_TABLE=virtual-environments
ROUTING_CONFIG_TABLE=routing-config
PRIORITIES_TABLE=alb-rule-priorities

# ALB Configuration
ALB_LISTENER_ARN=arn:aws:elasticloadbalancing:eu-west-1:123456789012:listener/app/...

# CloudFront Configuration
CLOUDFRONT_FUNCTION_NAME=virtual-env-header-injection
CLOUDFRONT_DISTRIBUTION_ID=EXAMPLEDISTID
